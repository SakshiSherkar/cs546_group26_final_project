<h1>Property Search Results</h1>
{{#each searchResults}}
<div class = "card">
    <div class="card-content">
    <div class = "image-container">
        <img src="{{this.images}}" alt="Property Image">
        <i class="favorite-icon fas fa-heart{{#if isFavorite}} active{{/if}}"
    </div>
    <div class = "property-details">
    <p>Address: ${{address}}</p>
    <p>Price: ${{price}}</p>
    {{#if favoriteCount}}
    <p class="favorite-count" >Favorite Count: {{favoriteCount}}</p>
    {{/if}}
    </div>
    </div>
</div>
{{/each}}
